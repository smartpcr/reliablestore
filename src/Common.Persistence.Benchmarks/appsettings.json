{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "Providers": {
        "InMemory": {
          "Capabilities": "Crud"
        },
        "ClusterRegistry": {
          "Capabilities": "Crud",
          "ApplicationName": "BenchmarkApp",
          "ServiceName": "BenchmarkSvc",
          "FallbackToLocalRegistry": true
        },
        "Esent": {
          "Capabilities": "Crud",
          "DatabasePath": "data/benchmark.db",
          "InstanceName": "BenchmarkInstance",
          "UseSessionPool": false
        }
      }
}